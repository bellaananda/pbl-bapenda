{"ast":null,"code":"import { Form } from 'vform';\nimport axios from 'axios';\nimport swal from 'sweetalert2';\nexport default {\n  name: 'categori-management',\n  data() {\n    return {\n      categories: {},\n      form: new Form({\n        id: '',\n        name: ''\n      }),\n      isFormCreateCategoriMode: true\n    };\n  },\n  methods: {\n    showModal() {\n      this.isFormCreateCategoriMode = true;\n      this.form.reset();\n      this.$root.$emit('bv::show::modal', 'modalKategori', '#btnShow');\n    },\n    hideModal() {\n      this.$root.$emit('bv::hide::modal', 'modalKategori', '#btnShow');\n    },\n    getCategories(page) {\n      if (typeof page === 'undefined') {\n        page = 1;\n      }\n      axios.get('http://localhost:8000/api/categories', {\n        params: {\n          page: page\n        }\n      }).then(data => {\n        this.categories = data.data;\n      });\n    },\n    // showModal() {\n    //   this.isFormCreateCategoriMode = true;\n    //   this.form.reset(); // v form reset\n    //   $('#modalKategori').modal('show'); // show modal\n    // },\n\n    createCategori() {\n      // request post\n      this.form.post('http://localhost:8000/api/categories', {}).then(() => {\n        this.$bvModal.hide(); // hide modal\n        this.getCategories();\n      }).catch(() => {\n        console.log('transaction fail');\n      });\n    },\n    editCategori(categori) {\n      this.isFormCreateCategoriMode = false;\n      this.form.reset(); // v form reset inputs\n      this.form.clear(); // v form clear errors\n      this.$bvModal.show(); // show modal\n      this.form.fill(categori);\n    },\n    updateCategori() {\n      this.form.put('http://localhost:8000/api/categories/' + this.form.id, {}).then(() => {\n        this.$bvModal.hide(); // hide modal\n        this.getCategories();\n      }).catch(() => {\n        console.log('transaction fail');\n      });\n    },\n    deleteCategori(id) {\n      // sweet alert confirmation\n      swal.fire({\n 