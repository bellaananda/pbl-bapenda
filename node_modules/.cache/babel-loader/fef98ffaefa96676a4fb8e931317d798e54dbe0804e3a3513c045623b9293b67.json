      createCategori() {\r\n          // request post\r\n          this.form.post('http://localhost:8000/api/categories', {\r\n          }).then(() => {\r\n            this.enable = true;\r\n            this.form = {};\r\n          }).catch(() => {\r\n            console.log('transaction fail');\r\n          });\r\n        },\r\n\r\n        simpanCategori(categori) {\r\n          this.categories.push({\r\n            'name': categori.name\r\n          });\r\n        },\r\n\r\n        editCategori(categori){\r\n          this.enable = false;\r\n          this.index = this.categories.indexOf(categori);\r\n          this.form.id = categori.id;\r\n          this.form.name = categori.name;\r\n        },\r\n\r\n        updateCategori(categori){\r\n          this.form.put('http://localhost:8000/api/categories/' + this.form.id, {\r\n          }).then(() => {\r\n            this.categories[this.index].id = categori.id;\r\n            this.categories[this.index].name = categori.name;\r\n          }).catch(() => {\r\n            console.log('transaction fail');\r\n          });\r\n        }, \r\n\r\n        deleteCategori(id) {\r\n          // sweet alert confirmation\r\n          swal.fire({\r\n            title: 'Ingin menghapus data?',\r\n            // text: \"You won't be able to revert this!\",\r\n            icon: 'warning',\r\n            showCancelButton: true,\r\n            confirmButtonColor: '#3085d6',\r\n            cancelButtonColor: '#d33',\r\n            confirmButtonText: 'Ya hapus data'\r\n          }).then((result) => {\r\n            // confirm delete?\r\n            if (result.value) {\r\n              // request delete\r\n              this.form.delete('http://localhost:8000/api/categories/' + id, {\r\n               }).then(() => {\r\n                // sweet alert success\r\n                swal.fire(\r\n                  'Deleted!',\r\n                  'Your file has been deleted.',\r\n                  'success'\r\n                )   \r\n  \r\n    