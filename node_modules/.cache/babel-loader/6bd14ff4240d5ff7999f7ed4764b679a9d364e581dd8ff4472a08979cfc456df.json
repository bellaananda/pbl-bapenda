{"ast":null,"code":"import { Form } from 'vform';\nimport axios from 'axios';\nimport swal from 'sweetalert2';\nexport default {\n  name: 'RuanganBapenda',\n  data() {\n    return {\n      rooms: {},\n      form: new Form({\n        id: '',\n        name: ''\n      }),\n      isFormCreateRoomsMode: true\n    };\n  },\n  methods: {\n    showModal() {\n      this.isFormCreateRoomsMode = true;\n      this.form.reset();\n      this.$root.$emit('bv::show::modal', 'modalRuang', '#btnShow');\n    },\n    hideModal() {\n      this.$root.$emit('bv::hide::modal', 'modalRuang', '#btnShow');\n    },\n    getRooms(page) {\n      if (typeof page === 'undefined') {\n        page = 1;\n      }\n      axios.get('http://localhost:8000/api/rooms', {\n        params: {\n          page: page\n        }\n      }).then(data => {\n        this.rooms = data.data;\n      });\n    },\n    // showModal() {\n    //   this.isFormCreateCategoriMode = true;\n    //   this.form.reset(); // v form reset\n    //   this.$refs['my-modal'].show() // show modal\n    // },\n    // hideModal() {\n    //   this.isFormCreateCategoriMode = false;\n    //   this.$refs['my-modal'].hide()\n    // },\n\n    createRoom() {\n      // request post\n      this.form.post('http://localhost:8000/api/rooms', {}).then(() => {\n        this.$bvModal.hide();\n        swal.fire({\n          icon: 'success',\n          title: 'rooms created successfully'\n        });\n        this.getRooms();\n      }).catch(() => {\n        console.log('transaction fail');\n      });\n    },\n    editRoom(room) {\n      this.isFormCreateRoomsMode = false;\n      this.form.reset(); // v form reset inputs\n      this.form.clear(); // v form clear errors\n      this.$bvModal.show(); // show modal\n      this.form.fill(room);\n    },\n    updateRoom() {\n      this.form.put('http://localhost:8000/api/rooms/' + this.form.id, {}).then(() => {\n        this.$bvModal.hide(); // hide modal\n        swal.fire({\n          icon: 'success',\n          title: 'User updated successfully'\n        });\n        this.getRooms();\n      }).catch(() => {\n        console.log('transaction fail');\n      });\n    },\n    deleteRoom(id) {\n      // sweet alert confirmation\n      swal.fire({\n        title: 'Ingin menghapus data?',\n        // text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Ya hapus data'\n      }).then(result => {\n        // confirm delete?\n        if (result.value) {\n          // request delete\n          this.form.delete('http://localhost:8000/api/rooms/' + id, {}).then(() => {\n            // sweet alert success\n            swal.fire('Deleted!', 'Your file has been deleted.', 'success');\n            this.getRooms();\n          }).catch(() => {\n            // sweet alert fail\n            swal.fire({\n              icon: 'error',\n              title: 'Oops...',\n              text: 'Something went wrong!',\n              footer: '<a href>Why do I have this issue?</a>'\n            });\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.getRooms();\n  },\n  mounted() {\n    console.log('Component mounted.');\n  }\n};","map":{"version":3,"names":["Form","axios","swal","name","data","rooms","form","id","isFormCreateRoomsMode","methods","showModal","reset","$root","$emit","hideModal","getRooms","page","get","params","then","createRoom","post","$bvModal","hide","fire","icon","title","catch","console","log","editRoom","room","clear","show","fill","updateRoom","put","deleteRoom","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","value","delete","text","footer","created","mounted"],"sources":["C:\\wamp64\\www\\pbl-front\\src\\views\\admin\\RuanganBapenda.vue"],"sourcesContent":["<template>\r\n  <div class=\"content-wrapper\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12 grid-margin title\">\r\n            <div class=\"row\">\r\n              <div class=\"col-12 align-items-center\">\r\n                <h3 class=\"font-weight-bold\">KELOLA RUANG BAPENDA SURAKARTA</h3>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12 grid-margin stretch-card\">\r\n            <div class=\"card\">\r\n              <div class=\"card-body\">\r\n                <p class=\"card-description float-right\">\r\n                  <a href=\"#\" ref=\"#btnShow\" class=\"btn btn-sm btn-primary btn-icon-text\" data-toggle=\"modal\" data-target=\"#modalTambah\">\r\n                    <i class=\"mdi mdi-plus btn-icon-prepend\"></i>\r\n                    Tambah\r\n                  </a>\r\n                </p>\r\n                <div class=\"table-responsive\">\r\n                  <table class=\"table table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th>No</th>\r\n                        <th>Ruang Agenda</th>\r\n                        <th></th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(room, index) in rooms.data\" :key=\"room.id\">\r\n                        <td>{{ index+1 }}</td>\r\n                        <td>{{ room.name }}</td>\r\n                        <td>\r\n                          <a href=\"\" class=\"btn btn-sm\" @click.prevent=\"editRoom(room)\" >\r\n                            <i class=\"mdi mdi-pencil btn-icon-prepend\"></i>\r\n                          </a>\r\n                          <a href=\"\" class=\"btn btn-sm\" @click.prevent=\"deleteRoom(room.id)\">\r\n                              <i class=\"mdi mdi-delete btn-icon-prepend\"></i>\r\n                          </a>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"modal fade\" id=\"modalTambah\" aria-labelledby=\"ModalLabel\">\r\n          <div class=\"modal-dialog\" role=\"document\">\r\n            <div class=\"modal-content modal-md\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"ModalLabel\">Form Tambah Posisi</h5>\r\n                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                  <span aria-hidden=\"true\">&times;</span>\r\n                </button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                  <form class=\"forms-sample\">\r\n                      <div class=\"form-group\">\r\n                        <label for=\"inputPositions\">Posisi</label>\r\n                        <input type=\"text\" class=\"form-control\" v-model=\"post_name\" id=\"inputPositions\" placeholder=\"Posisi Pegawai\">\r\n                        <!-- <div v-if=\"validation.name\" class=\"mt-2 alert alert-danger\">\r\n                          {{ validation.name[0] }}\r\n                        </div> -->\r\n                      </div>\r\n                      <button class=\"btn btn-primary mr-2\" @click=\"createPosisi\">Simpan</button>\r\n                      <button class=\"btn btn-light\" @click=\"modalTambah = false\">Batalkan</button>\r\n                  </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n</template>\r\n\r\n<script>\r\nimport { Form } from 'vform'\r\nimport axios from 'axios'\r\nimport swal from 'sweetalert2';\r\n\r\n  export default {\r\n      name: 'RuanganBapenda',\r\n      data() {\r\n        return {\r\n          rooms: {},\r\n          form: new Form({\r\n            id: '',\r\n            name: ''\r\n          }),\r\n          isFormCreateRoomsMode: true\r\n        }\r\n      },\r\n\r\n      methods: {\r\n        showModal() {\r\n          this.isFormCreateRoomsMode = true;\r\n          this.form.reset();\r\n          this.$root.$emit('bv::show::modal', 'modalRuang', '#btnShow')\r\n        },\r\n        hideModal() {\r\n          this.$root.$emit('bv::hide::modal', 'modalRuang', '#btnShow')\r\n        },\r\n\r\n        getRooms(page) {\r\n          if (typeof page === 'undefined') {\r\n            page = 1;\r\n          }\r\n  \r\n          axios.get('http://localhost:8000/api/rooms', {\r\n            params: {\r\n              page: page\r\n            }\r\n          }).then(data => {\r\n            this.rooms = data.data;\r\n          });     \r\n        },\r\n\r\n        // showModal() {\r\n        //   this.isFormCreateCategoriMode = true;\r\n        //   this.form.reset(); // v form reset\r\n        //   this.$refs['my-modal'].show() // show modal\r\n        // },\r\n        // hideModal() {\r\n        //   this.isFormCreateCategoriMode = false;\r\n        //   this.$refs['my-modal'].hide()\r\n        // },\r\n\r\n        createRoom() {\r\n          // request post\r\n          this.form.post('http://localhost:8000/api/rooms', {\r\n          }).then(() => {\r\n            this.$bvModal.hide()\r\n\r\n            swal.fire({\r\n              icon: 'success',\r\n              title: 'rooms created successfully'\r\n            })\r\n            this.getRooms();\r\n          }).catch(() => {\r\n            console.log('transaction fail');\r\n          });\r\n        },\r\n\r\n        editRoom(room){\r\n          this.isFormCreateRoomsMode = false;\r\n          this.form.reset(); // v form reset inputs\r\n          this.form.clear(); // v form clear errors\r\n          this.$bvModal.show() // show modal\r\n          this.form.fill(room);\r\n        },\r\n\r\n        updateRoom(){\r\n          this.form.put('http://localhost:8000/api/rooms/' + this.form.id, {\r\n          }).then(() => {\r\n            this.$bvModal.hide() // hide modal\r\n            swal.fire({\r\n              icon: 'success',\r\n              title: 'User updated successfully'\r\n            })\r\n            this.getRooms();\r\n          }).catch(() => {\r\n            console.log('transaction fail');\r\n          });\r\n        }, \r\n\r\n        deleteRoom(id) {\r\n          // sweet alert confirmation\r\n          swal.fire({\r\n            title: 'Ingin menghapus data?',\r\n            // text: \"You won't be able to revert this!\",\r\n            icon: 'warning',\r\n            showCancelButton: true,\r\n            confirmButtonColor: '#3085d6',\r\n            cancelButtonColor: '#d33',\r\n            confirmButtonText: 'Ya hapus data'\r\n          }).then((result) => {\r\n            // confirm delete?\r\n            if (result.value) {\r\n              // request delete\r\n              this.form.delete('http://localhost:8000/api/rooms/' + id, {\r\n               }).then(() => {\r\n                // sweet alert success\r\n                swal.fire(\r\n                  'Deleted!',\r\n                  'Your file has been deleted.',\r\n                  'success'\r\n                )   \r\n  \r\n                this.getRooms(); \r\n              }).catch(() => {\r\n                // sweet alert fail\r\n                swal.fire({\r\n                  icon: 'error',\r\n                  title: 'Oops...',\r\n                  text: 'Something went wrong!',\r\n                  footer: '<a href>Why do I have this issue?</a>'\r\n                })\r\n              }); \r\n            }\r\n          })\r\n        }\r\n      },\r\n\r\n      created() {\r\n        this.getRooms();\r\n      },\r\n      mounted() {\r\n        console.log('Component mounted.')\r\n      }\r\n    }\r\n</script>"],"mappings":"AAgFA,SAASA,IAAG,QAAS,OAAM;AAC3B,OAAOC,KAAI,MAAO,OAAM;AACxB,OAAOC,IAAG,MAAO,aAAa;AAE5B,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,IAAI,EAAE,IAAIN,IAAI,CAAC;QACbO,EAAE,EAAE,EAAE;QAC