{
  "name": "agenda-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "bootstrap": "^5.2.3",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "jquery": "^3.6.4",
    "jspdf": "^2.5.1",
    "laravel-vue-pagination": "^4.1.0",
    "popper.js": "^1.16.1",
    "sweetalert2": "^11.7.3",
    "vform": "^2.1.2",
    "vue": "^3.2.13",
    "vue-json-excel": "^0.3.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-dollar-sign": "^1.0.2",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "jquery": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "sourceType": "module"
    },
    "plugins": [
      "dollar-sign",
      "jquery"
    ],
    "rules": {
      "no-mixed-spaces-and-tabs": 0,
      "indent": "off",
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
